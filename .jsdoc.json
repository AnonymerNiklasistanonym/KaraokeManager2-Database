{
    "plugins": ["plugins/markdown"],
    "recurseDepth": 10,
    "source": {
        "exclude": [
            "node_modules",
            "docs"
        ],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": [
            "jsdoc",
            "closure"
        ]
    },
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": true
    },
    "linenums": true,
    "opts": {
        "encoding": "utf8",
        "recurse": true,
        "destination": "./docs/jsdoc/"
    }
}
